{
  "name": "@monobase/sdk",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./api": {
      "types": "./src/api.ts",
      "default": "./src/api.ts"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "default": "./src/utils/*.ts"
    },
    "./services/*": {
      "types": "./src/services/*.ts",
      "default": "./src/services/*.ts"
    },
    "./react/*": {
      "types": "./src/react/*",
      "default": "./src/react/*"
    }
  },
  "files": ["src"],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@daveyplate/better-auth-tanstack": "^1.3.6",
    "@monobase/api-spec": "workspace:*",
    "better-auth": "^1.3.7",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.0"
  },
  "devDependencies": {
    "@monobase/typescript-config": "workspace:*",
    "@types/bun": "^1.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.9.2"
  }
}